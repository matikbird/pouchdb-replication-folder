{
  "name": "pouchdb-replication-folder",
  "version": "1.0.0",
  "description": "Replicate PouchDB/CouchDB database with a shared folder (e.g. Dropbox, iCloud Drive ...etc)",
  "main": "index.js",
  "keywords": [
    "pouch",
    "pouchdb",
    "plugin",
    "dropbox",
    "icloud",
    "file",
    "replication",
    "couch",
    "couchdb"
  ],
  "scripts": {
    "test-node": "TEST_DB=testdb,http://localhost:5984/testdb istanbul test ./node_modules/mocha/bin/_mocha test/test.js",
    "test-browser": "./bin/test-browser.js",
    "jshint": "jshint -c .jshintrc *.js test/test.js",
    "test": "npm run jshint && ./bin/run-test.sh",
    "build": "mkdirp dist && npm run browserify && npm run min",
    "browserify": "browserify index.js | ./bin/es3ify.js | derequire > dist/pouchdb.replicate-folder.js",
    "min": "uglifyjs dist/pouchdb.replicate-folder.js -mc > dist/pouchdb.replicate-folder.min.js",
    "dev": "browserify test/test.js > test/test-bundle.js && npm run dev-server",
    "dev-server": "./bin/dev-server.js",
    "coverage": "npm test --coverage && istanbul check-coverage --lines 100 --function 100 --statements 100 --branches 100"
  },
  "author": "Francis Chong <francis@ignition.hk> (http://reality.hk/)",
  "license": "MIT",
  "dependencies": {
    "pouchdb-dump-cli": "^2.0.3",
    "pouchdb-load": "^1.4.3",
    "lie": "^2.6.0",
    "inherits": "~2.0.1",
    "argsarray": "0.0.1"
  },
  "devDependencies": {
    "bluebird": "^1.0.7",
    "browserify": "^9.0.8",
    "chai": "^1.9.1",
    "derequire": "^2.0.0",
    "es3ify": "^0.1.3",
    "http-server": "~0.5.5",
    "istanbul": "^0.2.7",
    "jshint": "^2.3.0",
    "mkdirp": "^0.5.0",
    "mocha": "^1.18.2",
    "phantomjs": "^1.9.7-5",
    "pouchdb": "pouchdb/pouchdb",
    "request": "^2.36.0",
    "sauce-connect-launcher": "^0.4.2",
    "selenium-standalone": "^3.0.2",
    "uglify-js": "^2.4.13",
    "watchify": "^3.1.0",
    "wd": "^0.2.21"
  }
}
